# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=release
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),release)
  hello1_config = release
  hello2_config = release
endif
ifeq ($(config),debug)
  hello1_config = debug
  hello2_config = debug
endif

PROJECTS := hello1 hello2

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

hello1:
ifneq (,$(hello1_config))
	@echo "==== Building hello1 ($(hello1_config)) ===="
	@${MAKE} --no-print-directory -C hello1 -f Makefile config=$(hello1_config)
endif

hello2:
ifneq (,$(hello2_config))
	@echo "==== Building hello2 ($(hello2_config)) ===="
	@${MAKE} --no-print-directory -C hello2 -f Makefile config=$(hello2_config)
endif

clean:
	@${MAKE} --no-print-directory -C hello1 -f Makefile clean
	@${MAKE} --no-print-directory -C hello2 -f Makefile clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  release"
	@echo "  debug"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   hello1"
	@echo "   hello2"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"