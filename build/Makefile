ifndef config
  config=release
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),release)
  hello1_config = release
  hello2_config = release
  openat_config = release
  dup2_config = release
  uid_config = release
  game_config = release
endif
ifeq ($(config),debug)
  hello1_config = debug
  hello2_config = debug
  openat_config = debug
  dup2_config = debug
  uid_config = debug
  game_config = debug
endif

PROJECTS := hello1 hello2 openat dup2 uid game

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

hello1:
ifneq (,$(hello1_config))
	@echo "==== Building hello1 ($(hello1_config)) ===="
	@${MAKE} --no-print-directory -C hello1 -f Makefile config=$(hello1_config)
endif

